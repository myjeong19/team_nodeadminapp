<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <title>INSPINIA | Main view</title>

  <link href="/css/bootstrap.min.css" rel="stylesheet" />
  <link href="/font-awesome/css/font-awesome.css" rel="stylesheet" />

  <link href="/css/animate.css" rel="stylesheet" />
  <link href="/css/style.css" rel="stylesheet" />

  <!--추가 스타일-->
</head>

<!--우측 콘텐츠 헤더영역 -->
<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>회원가입</h2>
    <ol class="breadcrumb">
      <li>
        <a href="/main">홈</a>
      </li>
      <li>
        <a>회원가입</a>
      </li>
      <li class="active">
        <strong>조회</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2"></div>
</div>

<!--우측 콘텐츠 영역 -->
<div class="wrapper wrapper-content">
  <form
    class="ibox-content m-b-sm border-bottom"
    action="/admin/modify/<%= admin.admin_member_id %>"
    method="post"
  >
    <div class="row">
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">관리자 고유번호</label>
          <input
            type="text"
            class="form-control"
            value="<%= admin.admin_member_id %>"
            name="admin_member_id"
            disabled
          />
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">소속회사 코드</label>
          <input
            type="text"
            class="form-control"
            value="<%= admin.company_code %>"
            name="company_code"
          />
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">관리자 계정 아이디</label>
          <input
            type="text"
            class="form-control"
            value="<%= admin.admin_id %>"
            name="admin_id"
          />
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">사용 여부</label>
          <input
            type="text"
            class="form-control"
            value="<%= admin.used_yn_code %>"
            name="used_yn_code"
          />
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">관리자 이름</label>
          <input
            type="text"
            class="form-control"
            value="<%= admin.admin_name %>"
            name="admin_name"
          />
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">관리자 전화번호</label>
          <input
            type="text"
            class="form-control"
            value="<%= admin.telephone %>"
            name="telephone"
          />
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">등록자 아이디</label>
          <input
            type="text"
            class="form-control"
            value="<%= admin.reg_user_id %>"
            name="reg_user_id"
            disabled
          />
        </div>
      </div>

      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">등록일시</label>
          <input
            type="date"
            class="form-control"
            value="<%= admin.reg_date %>"
            disabled
            name="reg_date"
          />
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">수정자 아이디</label>
          <input
            type="text"
            class="form-control"
            value="<%= admin.edit_user %>"
            name="edit_user"
          />
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label class="control-label">수정일시</label>
          <input
            type="date"
            class="form-control"
            value="<%= admin.edit_date %>"
            name="edit_date"
          />
        </div>
      </div>
    </div>

    <div class="text-center">
      <button type="submit" class="btn btn-primary" name="action" value="save">
        저장
      </button>
      <button type="submit" name="action" value="delete">삭제</button>
      <a href="/admin/list" class="btn btn-info">목록</a>
    </div>
  </form>
</div>
